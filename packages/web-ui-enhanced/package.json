{
  "name": "@swarm-flow/web-ui-enhanced",
  "version": "1.0.0",
  "description": "Enhanced web UI with real-time monitoring, agent visualization, and performance analytics",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run build:assets",
    "build:assets": "webpack --mode production",
    "dev": "concurrently \"tsc --watch\" \"webpack-dev-server --mode development\"",
    "test": "jest",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "format": "prettier --write src/**/*.{ts,tsx,css,scss}"
  },
  "keywords": [
    "web-ui",
    "monitoring",
    "visualization",
    "real-time",
    "dashboard",
    "swarm"
  ],
  "author": "Swarm Flow Team",
  "license": "MIT",
  "dependencies": {
    "@swarm-flow/coordination-enhanced": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "socket.io-client": "^4.7.0",
    "three": "^0.158.0",
    "@react-three/fiber": "^8.15.0",
    "@react-three/drei": "^9.88.0",
    "recharts": "^2.8.0",
    "antd": "^5.12.0",
    "styled-components": "^6.1.0",
    "framer-motion": "^10.16.0",
    "d3": "^7.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/d3": "^7.4.0",
    "@types/three": "^0.158.0",
    "@types/jest": "^29.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.89.0",
    "webpack-dev-server": "^4.15.0",
    "webpack-cli": "^5.1.0",
    "ts-loader": "^9.5.0",
    "css-loader": "^6.8.0",
    "style-loader": "^3.3.0",
    "sass-loader": "^13.3.0",
    "concurrently": "^8.2.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.16.0",
    "@testing-library/user-event": "^14.0.0",
    "identity-obj-proxy": "^3.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0"
